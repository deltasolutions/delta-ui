{
  "name": "delta-data-chest",
  "version": "0.3.3",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "start-storybook -c docs -p 6006 --ci",
    "doc": "build-storybook -c docs -o dist/docs",
    "lint": "npm run lint:static && npm run lint:format",
    "lint:format": "prettier ./{lib,docs}/**/*.{ts,tsx} --write",
    "lint:static": "eslint ./{lib,docs}/**/*.{ts,tsx} --fix",
    "release": "delta-release"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@types/node": "^16.11.4",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "nats.ws": "^1.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsup": "^5.10.3"
  },
  "peerDependencies": {
    "nats.ws": "^1.4.1",
    "react": ">=16.13.1",
    "restyler": ">=0.30.0"
  },
  "peerDependenciesMeta": {
    "nats.ws": {
      "optional": true
    }
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "platform": "browser",
    "format": [
      "cjs",
      "esm"
    ],
    "entryPoints": [
      "lib/index.tsx"
    ]
  }
}
