  "license": "MIT",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "source": "lib/index.tsx",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "prebuild": "rimraf build",
