    "@types/node": "^16.11.4",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "nats.ws": "^1.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "nats.ws": "^1.4.1",
    "react": ">=16.13.1",
    "restyler": ">=0.30.0"
  },
  "peerDependenciesMeta": {
    "nats.ws": {
      "optional": true
