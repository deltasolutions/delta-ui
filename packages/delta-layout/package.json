{
  "name": "delta-layout",
  "version": "0.2.4",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "start-storybook -c docs -p 6006 --ci",
    "doc": "build-storybook -c docs -o dist/docs",
    "lint": "npm run lint:static && npm run lint:format",
    "lint:format": "prettier ./{lib,docs}/**/*.{ts,tsx} --write",
    "lint:static": "eslint ./{lib,docs}/**/*.{ts,tsx} --fix",
    "release": "delta-release"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.1.0",
    "@emotion/react": "^11.5.0",
    "@storybook/addon-docs": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@types/node": "^16.11.4",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "react": "^16.13.1",
    "react-dom": "^16.14.0",
    "react-icons": "^4.3.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@theme-ui/core": "^0.11.3",
    "react": ">=16.13.1",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": ">=16.14.0",
    "react-i18next": "^11.13.0",
    "restyler": ">=0.30.0",
    "restyler-theme-delta": ">=0.1.0"
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "platform": "browser",
    "format": [
      "cjs",
      "esm"
    ],
    "entryPoints": [
      "lib/index.tsx"
    ]
  }
}
